var original_param = '';
var new_param = '';

function getOperations() {
	$('#response').html('');
	var ajax_url = 'http://<?php echo HTTP_HOST ?>/ajax/<?php echo DOMAIN_NAME ?>/index.php';
	$.ajax({
		data : {'wsdl':$('#wsdl').val(),
				'endpoint':$('#endpoint').val(),
				'version':$('#version').val(),
				'style':$('#style').val(),
				'encoding':$('#encoding').val(),
				
			<?php for($i=0; $i<20; $i++){ ?>
			'parameter_name_pnv_<?php echo $i ?>':$('#parameter_name_pnv_<?php echo $i ?>').val(),
			'parameter_value_pnv_<?php echo $i ?>':$('#parameter_value_pnv_<?php echo $i ?>').val(),
			
			<?php } ?>
			
				'operation':$('#operation').val()
		},	
		url : ajax_url,
		success : function(response) {
			$('#response').html(response).show();
		}
	});
}

function createNewParam(){

	if(original_param == '') {
		original_param = $('.parameters').html();
	} 
	
	$('.add_param').remove();
	$('.ap').remove();
	
	$('.parameters').first('tr').before('<tr class="parameters">' + original_param + '</tr>');
		
}


