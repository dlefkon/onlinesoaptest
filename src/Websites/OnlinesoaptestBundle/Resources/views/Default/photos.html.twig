{% extends 'OnlinesoaptestBundle::layout.html.twig' %}

			{% block content %} 
			
	<style type="text/css">
	
		img { 
		
			width:auto;
			
		}
		
	</style>
				{% if 'supersoap' == app.request.get('name')  %}
			
					<a href="{{ app.request.get('href_url') }}" target="_blank">
						<img alt="A photo of {{ app.request.get('name') }}" src="/bundles/onlinesoaptest/images/soap/supersoap.jpg">
					</a>
					
				{% elseif 'structure' == app.request.get('name')  %}
			
					<a href="{{ app.request.get('href_url') }}" target="_blank">
						<img alt="A photo of {{ app.request.get('name') }}" src="/bundles/onlinesoaptest/images/soap/structure.gif">
					</a>
					
				{% elseif 'flow' == app.request.get('name')  %}
			
					<a href="{{ app.request.get('href_url') }}" target="_blank">
						<img alt="A photo of {{ app.request.get('name') }}" src="/bundles/onlinesoaptest/images/soap/flow.gif">
					</a>
				
				{% elseif 'layers' == app.request.get('name')  %}
			
					<a href="{{ app.request.get('href_url') }}" target="_blank">
						<img alt="A photo of {{ app.request.get('name') }}" src="/bundles/onlinesoaptest/images/soap/layers.gif">
					</a>
					
				{% endif %}
				
			<script>
	
				$(".image_link").click( function() {
	
					console.log($(this).prop('id'));
					
					switch( $(this).prop('id') ) {
					
						case 'supersoap_href':
							window.open( "http://itknowledgeexchange.techtarget.com/qa-processes/page/3/" );
									
					
					}
				
				});
			
			</script>
		
			{% endblock content %}